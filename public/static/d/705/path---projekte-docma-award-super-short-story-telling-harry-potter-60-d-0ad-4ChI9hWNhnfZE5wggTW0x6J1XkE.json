llen","time":"1 Woche, Mai 2017","cover":{"childImageSharp":{"fluid":{"base64":"data:image/jpeg;base64,","aspectRatio":1.4138438880706923,"src":"/static/docma_award-eaab58f744c6516deb08893045393b90-ec934.jpg","srcSet":"/static/docma_award-eaab58f744c6516deb08893045393b90-77d95.jpg 480w,\n/static/docma_award-eaab58f744c6516deb08893045393b90-b7fa3.jpg 960w,\n/static/docma_award-eaab58f744c6516deb08893045393b90-ec934.jpg 1920w","srcWebp":"/static/docma_award-eaab58f744c6516deb08893045393b90-198ec.webp","srcSetWebp":"/static/docma_award-eaab58f744c6516deb08893045393b90-3cd38.webp 480w,\n/static/docma_award-eaab58f744c6516deb08893045393b90-b74eb.webp 960w,\n/static/docma_award-eaab58f744c6516deb08893045393b90-198ec.webp 1920w","sizes":"(max-width: 1920px) 100vw, 1920px"},"resize":{"src":"/static/docma_award-eaab58f744c6516deb08893045393b90-d260a.jpg"}}}},"fields":{"slug":"/projekte/docma-award-super-short-story-telling-harry-potter","sourceInstanceName":"projekte"}}},"pageContext":{"slug":"/projekte/docma-award-super-short-story-telling-harry-potter","left":{"fields":{"slug":"/projekte/elitecases-website-fur-virtuelle-spiele-gegenstande"},"frontmatter":{"title":"Website für virtuelle Spiele-Gegenstände","cover":{"childImageSharp":{"resize":{"src":"/static/elitecases-2f4ad55eca634abb60e4eaea64a6be83-735d8.jpg"}}}}},"right":{"fields":{"slug":"/projekte/combmaster-i-os-spiele-app"},"frontmatter":{"title":"iOS Spiele-App","cover":{"childImageSharp":{"resize":{"src":"/static/combmaster-88995ecd4373b3bd929983cf4fa3c9a1-735d8.jpg"}}}}}}}